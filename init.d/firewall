#!/bin/bash
# description: Firewall service for adding and removing rules

# Source function library.
. /etc/init.d/functions

start() {
	# start code
}

stop() {
	# stop code
}

case "$1" in 
	start)
		start
		;;
	stop)
		stop
		;;
	restart)
		stop
		start
		;;
	*)
		echo "Usage: $0 {start|stop|status|restart}"
esac

exit 0
